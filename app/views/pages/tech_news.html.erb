<div class="inner cover">
  <h1 class="cover-heading">Let's Get Techy</h1>
  <div class="row">
    <% @tweets.inspect do |tweet|%>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= tweet_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>


